    "# heap 자료구조를 만들어보자\n",
    "import heapq\n",
    "\n",
    "arr = [1,5,3,8,10,2,11]\n",
    "\n",
    "# 최소힙\n",
    "#heapq.heapify(arr)\n",
    "\n",
    "# 최대힙\n",
    "temp = []\n",
    "for num in arr:\n",
    "    heapq.heappush(temp, (-num,num)) # 특정 arr에, ㅌㅡㄱㅈㅓㅇㄱㅏㅂㅅㅇㅡㄹ\n",
    "heapq.heappush(temp, (-7,7))\n",
    "temp"

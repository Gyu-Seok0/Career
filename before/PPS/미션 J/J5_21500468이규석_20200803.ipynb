      "4\n"
     ]
    }
   ],
   "source": [
    "from collections import deque\n",
    "card = deque()\n",
    "for i in range(N):\n",
    "    card.append(i+1)\n",
    "\n",
    "while(len(card) != 1):\n",
    "    card.popleft()\n",
    "    card.append(card.popleft())\n",
    "print(card[0])"
   ]
  },
